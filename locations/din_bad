	end

	if bfA > 0 and boydayA ! daystart and hour > 5 and hour < 20 and ringA = 0:
		if bfB = 1:bfB = 0
		if bfC = 1:bfC = 0

		if vnesh < vneshBoyA:
			bfA -= 1
			if bfA = 0:
				'<b><font color="red">Your guy does not call you, he probably dumped you.</font></b>'
			else
				'Your boyfriend <<$boyA>> has not called you.'
			end
