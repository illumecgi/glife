	if player_avatar = 1:
		view 'avatar.jpg'
	elseif glass <= 0:
		if shorthair = 0:
			if hcol = 0 and curly <= 0:view 'images/picb/hcol.jpg'
			if hcol = 1 and curly <= 0:view 'images/picb/hcol1.jpg'
