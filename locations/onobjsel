			if hcol = 2 and curly <= 0:view 'images/picb/nerd2.jpg'
			if hcol = 3 and curly <= 0:view 'images/picb/nerd3.jpg'
			if hcol = 0 and curly > 0:view 'images/picb/nerdcurly.jpg'
			if hcol = 1 and curly > 0:view 'images/picb/nerdcurly1.jpg'
			if hcol = 2 and curly > 0:view 'images/picb/nerdcurly2.jpg'
			if hcol = 3 and curly > 0:view 'images/picb/nerdcurly3.jpg'
		elseif shorthair = 1:
			if hcol = 0:view 'images/picb/SHhcolglass.jpg'
			if hcol = 1:view 'images/picb/SHhcolglass1.jpg'
			if hcol = 2:view 'images/picb/SHhcolglass2.jpg'
			if hcol = 3:view 'images/picb/SHhcolglass3.jpg'
		elseif glass = 2:
			if hcol = 0 and curly <= 0:view 'images/picb/hcolglass.jpg'
			if hcol = 1 and curly <= 0:view 'images/picb/hcolglass1.jpg'
			if hcol = 2 and curly <= 0:view 'images/picb/hcolglass2.jpg'
			if hcol = 3 and curly <= 0:view 'images/picb/hcolglass3.jpg'
			if hcol = 0 and curly > 0:view 'images/picb/hcolglasscurly.jpg'
			if hcol = 1 and curly > 0:view 'images/picb/hcolglasscurly1.jpg'
			if hcol = 2 and curly > 0:view 'images/picb/hcolglasscurly2.jpg'
			if hcol = 3 and curly > 0:view 'images/picb/hcolglasscurly3.jpg'
		end
	end
}

$objbody = {
	if musle >= salo:
		!!muscular type
		if bedra < 60:view 'images/body/fit.jpg'
		if bedra >= 60 and bedra < 70:view 'images/body/fit1.jpg'
		if bedra >= 70 and bedra < 80:view 'images/body/fit2.jpg'
		if bedra >= 80 and bedra < 90:view 'images/body/fit3.jpg'
		if bedra >= 90 and bedra < 100:view 'images/body/fit4.jpg'
		if bedra >= 100 and bedra < 110:view 'images/body/fit5.jpg'
		if bedra >= 110 and bedra < 120:view 'images/body/fit6.jpg'
		if bedra >= 120:view 'images/body/fit7.jpg'
	else
		!!bold type
		! Updated to be consistent with current images
		if bedra < 60:
			view 'images/body/fat.jpg'
		elseif bedra < 70:
			view 'images/body/fat1.jpg'
		elseif bedra < 80:
			view 'images/body/fat2.jpg'
		elseif bedra < 90:
			view 'images/body/fat3.jpg'
		elseif bedra < 100:
			view 'images/body/fat4.jpg'
		elseif bedra < 110:
			view 'images/body/fat5.jpg'
		elseif bedra < 120:
			view 'images/body/fat6.jpg'
		else
			view 'images/body/fat7.jpg'
		end
	end
}

