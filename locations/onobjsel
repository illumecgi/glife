$din_cheat_menu_time = {
	gs'stat'
	clr
	pl'<center><a href="exec:dynamic $objcheat_menu"><b>Cheat menu</b></a></center>'
	pl''
	pl'Right now <<$week>> <<day>> number, <<$month>>, <<year>> year.'
	pl'<a href="exec:dynamic $din_cheat_menu_week">Select Day of the Week</a>'
	pl'<a href="exec:day += 1 & dynamic $din_cheat_menu_time">+1 Day</a>'
	pl'<a href="exec:day -= 1 & dynamic $din_cheat_menu_time">-1 Day</a>'
	pl'<a href="exec:dynamic $din_cheat_menu_month">Select Month</a>'
	pl'<a href="exec:year += 1 & dynamic $din_cheat_menu_time">+1 Year</a>'
	pl'<a href="exec:year -= 1 & dynamic $din_cheat_menu_time">-1 Year</a>'
	if hour < 10:
    if minut > 9:pl'Right now <b>0<<hour>> : <<minut>></b>'
    if minut < 10:pl'Right now <b>0<<hour>> : 0<<minut>></b>'
	end
	if hour > 9:
    if minut > 9:pl'Right now <b><<hour>> : <<minut>></b>'
    if minut < 10:pl'Right now <b><<hour>> : 0<<minut>></b>'
	end
	$SS=''
	pl'Watch:'
	if hour>0:$SS='<a href="exec:hour -= 1 & dynamic $din_cheat_menu_time">-1</a> ' else $SS='    '
	if hour!0:$SS=$SS+'<a href="exec:hour = 0 & dynamic $din_cheat_menu_time">0</a> ' else $SS=$SS+'   '
	if hour<23:$SS=$SS+'<a href="exec:hour += 1 & dynamic $din_cheat_menu_time">+1</a>'
	pl $SS
	$SS=''
	pl'Minutes:'
	if minut>0:$SS='<a href="exec:minut -= 15 & dynamic $din_cheat_menu_time">-15</a> ' else $SS='    '
	if minut>0:$SS='<a href="exec:minut -= 1 & dynamic $din_cheat_menu_time">-1</a> ' else $SS='    '
	if minut!0:$SS=$SS+'<a href="exec:minut = 0 & dynamic $din_cheat_menu_time">0</a> ' else $SS=$SS+'   '
	if minut<59:$SS=$SS+'<a href="exec:minut += 1 & dynamic $din_cheat_menu_time">+1</a>'
	if minut<59:$SS=$SS+'<a href="exec:minut += 15 & dynamic $din_cheat_menu_time">+15</a>'
	pl $SS
}

