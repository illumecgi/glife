				hornystat = 0
				if pirsF = 1:hornystat += 1
				if pirsG = 1:hornystat += 1
				if pirsGL = 1:hornystat += 1
				hornyStat = hornystat * 4					
				if pirsA = 1:hornystat += 1
				if pirsB = 1:hornystat += 1
				if pirsC = 1:hornystat += 1
				if pirsD = 1:hornystat += 1
				if pirsE = 1:hornystat += 1
				if pirsN = 1:hornystat += 1
				set horny += hornyStat / 4 + rand(1,4) <= hornyStat mod 4 & ! Apply 1/4 of value on average
			end

			! WD: Clothing
			if Sexual = 1 and horny < 70:set horny += rand(0, 2)
			if glamur = 1 and horny < 50:set horny += rand(0, 1)
		end
			
!!SECTION: Bimbo (15 minute interval update checks)
		if bimbolevel = 2:
			if manna < mannamax:manna += ((manna / 100) * (rand(0,1)))
			if arousal < 25:arousal = 25
			if arousal < 100:arousal += 1
		end
		if bimbolevel = 3:
			if (manna < mannamax):manna += ((manna / 100) * (rand(0,2)))
			if arousal < 50:arousal = 50
			if arousal < 100:arousal += 2
		end
!! level four, adds supernatural-style succubus abilities. May be removed or spun off into its own TF or obtained through special means unlike the rest.
!! Also planning to add much more severe and rapidly advancing penalties for not having sex to offset bonuses
		if bimbolevel = 4:
			if arousal < 80:arousal = 80
			if arousal < 100:arousal += 2
			if cumlip > 0:sexnutrition += cumlip and cumlip = 0 and absorbflag = 1
			if swallow > 0:sexnutrition += swallow * 2 and swallow = 0 and absorbflag = 1
			if cumpussy > 0:sexnutrition += cumpussy * 4 and cumpussy = 0 and absorbflag = 1
			if cumanus > 0:sexnutrition += cumanus * 4 and cumanus = 0 and absorbflag = 1
